algoritmo "funcaoS"
var
   n1,n2,s:inteiro
funcao soma(x,y:inteiro):inteiro
inicio
      retorne x+y
fimfuncao
inicio
      escreva("Digite o primeiro valor: ")
      leia(n1)
      escreva("Digite o segundo valor: ")
      leia(n2)
      s<- soma(n1,n2)
      escreval(n1," + ",n2," = ",s)
fimalgoritmo
