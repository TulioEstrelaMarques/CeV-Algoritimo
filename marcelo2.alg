algoritmo "116"
var
quant:inteiro
num : real
inicio
   quant<-0
      escreval(" digite um numero")
      leia(num)
     enquanto (num > 1) faca
              num <- (num/2)
              quant <- quant+1
     fimenquanto
     escreval("o resultado da ultima divisao foi: ",num)
     escreval("a quantidade de divisao foi: ",quant)

fimalgoritmo
